{
    "manifest_version": 3,
    "name": "Fidelity Positions Exporter",
    "version": "1.1",
    "description": "Adds a Quick Download button to export Fidelity positions data to CSV.",
    "permissions": [],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://digital.fidelity.com/ftgw/digital/portfolio/positions",
                "https://digital.fidelity.com/ftgw/digital/portfolio/summary"
            ],
            "js": ["content.js"]
        }
    ]
}
